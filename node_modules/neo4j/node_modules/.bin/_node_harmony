#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  --harmony "$basedir/../streamline/bin/_node_harmony" "$@"
  ret=$?
else 
  node  --harmony "$basedir/../streamline/bin/_node_harmony" "$@"
  ret=$?
fi
exit $ret
